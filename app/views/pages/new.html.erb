<% @page_title = "New page" %>

<%= link_to("<< Back to List", pages_path, :class => 'back_list')%>
<div class="pages new">
	<h2>Create Page</h2>
	<%= form_for(@page) do |f|%>

		<%= render(:partial => 'form', :locals => {:f => f}) %>

		<div class="form-buttons">
			<%= f.submit("Create Page") %>
		</div>
	<% end %>
</div>
